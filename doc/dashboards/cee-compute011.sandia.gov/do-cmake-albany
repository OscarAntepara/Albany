
rm -rf CMake* 
TRILINSTALLDIR=
MESH_DEP_ON_PARAMS=TRUE
BUILTDYPE=RELEASE
FPE_CHECK=FALSE


cmake \
    -D ALBANY_TRILINOS_DIR:PATH=${TRILINSTALLDIR} \
    -D ENABLE_LANDICE:BOOL=ON \
    -D ENABLE_UNIT_TESTS:BOOL=ON \
    -D ENABLE_LARGE_TESTS:BOOL=ON \
    -D ENABLE_DEMO_PDES:BOOL=ON \
    -D ENABLE_STRONG_FPE_CHECK:BOOL=ON \
    -D ENABLE_MESH_DEPENDS_ON_PARAMETERS:BOOL=${MESH_DEP_ON_PARAMS} \
    -D CMAKE_BUILD_TYPE:STRING=${BUILDTYPE} \
    -D ENABLE_STRONG_FPE_CHECK:BOOL=${FPE_CHECK} \
    -D ENABLE_OMEGAH:BOOL=ON \
..
